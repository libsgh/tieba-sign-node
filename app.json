{
  "name": "tieba-sign-node",
  "description": "贴吧云签节点",
  "keywords": [
    "tieba-sign"
  ],
  "env": {
    "TZ": {
      "description": "时区，请勿修改",
      "value": "Asia/Shanghai"
    },
    "LANG": {
      "description": "编码格式，请勿修改",
      "value": "en_US.UTF-8"
    },
    "SERVER_NAME": {
      "description": "服务器节点名称，与app名称一致",
      "value": "toolsbox-x"
    }
  },
  "addons": [{
    "plan": "heroku-postgresql"
  }],
  "website": "http://github.com/libsgh/tieba-sign-node",
  "repository": "http://github.com/libsgh/tieba-sign-node"
}